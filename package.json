    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "cypress": "^14.4.1",
    "eslint": "^9.14.0",
